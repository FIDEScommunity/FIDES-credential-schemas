{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "UpperSecondaryEducationCertificate",
  "type": "object",
  "required": [
    "@context",
    "type",
    "credentialSubject",
    "issuer",
    "issuanceDate"
  ],
  "properties": {
    "@context": {
      "type": "array"
    },
    "type": {
      "type": "array"
    },
    "issuer": {
      "type": [
        "string",
        "object"
      ]
    },
    "issuanceDate": {
      "type": "string"
    },
    "credentialSubject": {
      "type": "object",
      "required": [
        "id",
        "dateOfBirth",
        "familyName",
        "givenName",
        "awardingDate",
        "learningAchievement",
        "eqfLevel",
        "awardingBody"
      ],
      "properties": {
        "id": {
          "type": "string"
        },
        "dateOfBirth": {
          "type": "string"
        },
        "familyName": {
          "type": "string"
        },
        "givenName": {
          "type": "string"
        },
        "awardingDate": {
          "type": "string"
        },
        "learningAchievement": {
          "type": "string"
        },
        "eqfLevel": {
          "type": "string"
        },
        "awardingBody": {
          "type": "string"
        }
      }
    },
    "proof": {
      "type": "object"
    }
  }
}