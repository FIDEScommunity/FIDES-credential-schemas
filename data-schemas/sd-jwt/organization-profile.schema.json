{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "SD-JWT OrganizationProfile Claims",
  "type": "object",
  "required": [
    "legal_name",
    "address_country"
  ],
  "properties": {
    "legal_name": {
      "type": "string"
    },
    "brand": {
      "type": "string"
    },
    "url": {
      "type": "string",
      "format": "uri"
    },
    "telephone": {
      "type": "string"
    },
    "email": {
      "type": "string",
      "format": "email"
    },
    "postal_code": {
      "type": "string"
    },
    "street_address": {
      "type": "string"
    },
    "address_locality": {
      "type": "string"
    },
    "address_country": {
      "type": "string"
    },
    "latitude": {
      "type": "number"
    },
    "longitude": {
      "type": "number"
    },
    "logo": {
      "type": "string",
      "format": "uri"
    }
  }
}