{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "SD-JWT CriminalRecordCheck Claims",
  "type": "object",
  "required": [
    "given_name",
    "family_name",
    "date_of_birth",
    "check_result",
    "check_type",
    "jurisdiction",
    "issued_date"
  ],
  "properties": {
    "given_name": {
      "type": "string"
    },
    "family_name": {
      "type": "string"
    },
    "date_of_birth": {
      "type": "string",
      "format": "date"
    },
    "check_result": {
      "type": "string",
      "enum": [
        "Clear",
        "Not Clear"
      ]
    },
    "check_type": {
      "type": "string"
    },
    "jurisdiction": {
      "type": "string"
    },
    "issued_date": {
      "type": "string",
      "format": "date"
    }
  }
}